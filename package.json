{
  "name": "forsta-message-vault",
  "version": "0.6.3",
  "author": "Forsta Inc.",
  "description": "Forsta Message Vault",
  "engines": {
    "node": "8"
  },
  "repository": "ForstaLabs/forsta-message-vault",
  "keywords": [
    "forsta",
    "e-discovery",
    "bot"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ForstaLabs/forsta-message-vault/issues"
  },
  "homepage": "https://github.com/ForstaLabs/forsta-message-vault#readme",
  "scripts": {
    "postinstall": "SKIP_PACKAGES=1 make",
    "start": "node server"
  },
  "bin": {
    "message-vault": "./server/index.js"
  },
  "build": {
    "appId": "io.forsta.message-vault",
    "productName": "Forsta Message Vault",
    "artifactName": "${name}-${version}-${os}.${ext}",
    "files": [
      "dist",
      "server",
      "electron",
      "!electron/downloads",
      "!electron/pgdata",
      "!electron/extraFiles",
      "!electron/buildResources",
      {
        "from": "electron/downloads/${os}/pgsql",
        "to": "pgsql"
      }
    ],
    "extraFiles": [
      {
        "from": "electron/extraFiles/${os}",
        "to": "resources/app/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asar": false,
    "asarUnpack": [
      "electron/pgsql"
    ],
    "directories": {
      "output": "builds",
      "buildResources": "electron/buildResources"
    }
  },
  "main": "./electron/main.js",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "browserify": "^14.5.0",
    "csv-stringify": "2.0.0",
    "envify": "^4.1.0",
    "express": "4.16.2",
    "grunt": "1.0.1",
    "grunt-browserify": "^5.2.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-sass": "1.0.0",
    "grunt-contrib-uglify-es": "git://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "grunt-env": "^0.4.4",
    "heroku-ssl-redirect": "0.0.4",
    "javascript-opentimestamps": "^0.2.7",
    "jquery": "3.5.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "librelay": "4.5.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "morgan": "1.9.1",
    "object-hash": "1.3.0",
    "swearjar": "^0.2.0",
    "uuid": "^3.1.0",
    "vue": "^2.5.9",
    "vue-focus": "^2.1.0",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "electron": "2.0.8",
    "electron-builder": "20.13.3",
    "eslint": "4.18.2",
    "grunt-contrib-watch": "1.0.0",
    "semantic-ui": "2.3.0",
    "semantic-ui-vue": "^0.2.10"
  }
}
